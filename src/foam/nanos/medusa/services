p({
  "class": "foam.nanos.boot.NSpec",
  "name": "medusaEntryDAO",
  "lazy": true,
  "serve": true,
  "serviceScript": `
    return new foam.dao.EasyDAO.Builder(x)
      .setOf(foam.nanos.medusa.MedusaEntry.getOwnClassInfo())
      .setGuid(true)
      .setInnerDAO((foam.dao.DAO) x.get("localMedusaEntryDAO"))
      .setCluster(false)
      .setRuler(false)
      .setEnableInterfaceDecorators(false)
      .setDecorator(new foam.nanos.medusa.MedusaEntryConsensusDAO.Builder(x)
        .setDelegate(new foam.nanos.medusa.MedusaEntryRouterDAO.Builder(x)
          .setDelegate(new foam.dao.NullDAO(x, foam.nanos.medusa.MedusaEntry.getOwnClassInfo()))
          .buid())
        .build())
      .build();
  `,
  "client": "{\"of\":\"foam.nanos.medusa.MedusaEntry\"}"
  // Index: global index.
})

